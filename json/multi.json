{
    "title":"Calendar (multi)",
    "description":"Calendar offers multiple rendering timelines to navigate through time (swipe up down).",
    "oml":{
        "selector:Calendar":"{CONFIG}"
    },
    "demoml":{
        "#instance:Calendar":{
            "format":"YYYY-MM-DD HH:mm:ss",
            "layouts":[
                {"layout":{"name":"CENTURY_DECADES"}, "engine":{"name":"DECADE_NUMBER", "config":false}},
                {"layout":{"name":"CENTURY_YEARS"}, "engine":{"name":"YEAR_NUMBER", "config":false}},
                {"layout":{"name":"DECADE_YEARS"}, "engine":{"name":"YEAR_NUMBER", "config":false}},
                {"layout":{"name":"DECADE_MONTHS"}, "engine":{"name":"MONTH_NUMBER", "config":false}}, 
                {"layout":{"name":"YEAR_MONTHS"}, "engine":{"name":"MONTH_NUMBER", "config":false}}, 
                {"layout":{"name":"YEAR_WEEKS"}, "engine":{"name":"WEEK_NUMBER", "config":false}},            
                {"layout":{"name":"MONTH_WEEKS"}, "engine":{"name":"WEEK_NUMBER", "config":false}},  
                {"layout":{"name":"MONTH_DAYS"}, "engine":{"name":"DAY_NUMBER", "config":false}},
                {"layout":{"name":"WEEK_DAYS"}, "engine":{"name":"DAY_NUMBER", "config":false}},                
                {"layout":{"name":"DAY"}, "engine":{"name":"DAY_NUMBER", "config":false}},
                {"layout":{"name":"DAY_HOURS"}, "engine":{"name":"CELL_VALUE", "config":{"format":"HH:[00]"}}},
                {"layout":{"name":"DAY_WORK_HOURS"}, "engine":{"name":"CELL_VALUE", "config":{"format":"HH:[00]"}}}
            ]
        }
    },
    "doc:OSE":"{{json _calendar}}"
}