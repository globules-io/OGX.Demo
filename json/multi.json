{
    "title":"Calendar (multi)",
    "description":"Calendar offers multiple rendering timelines to navigate through time (swipe up down).",
    "oml":{
        "selector:Calendar":"{CONFIG}"
    },
    "demoml":{
        "#instance:Calendar":{
            "format":"YYYY-MM-DD HH:mm:ss",
            "layouts":[
                {"layout":"CENTURY_DECADES", "engine":"DECADE_NUMBER", "config":false},
                {"layout":"CENTURY_YEARS", "engine":"YEAR_NUMBER", "config":false},
                {"layout":"DECADE_YEARS", "engine":"YEAR_NUMBER", "config":false},
                {"layout":"DECADE_MONTHS", "engine":"MONTH_NUMBER", "config":false}, 
                {"layout":"YEAR_MONTHS", "engine":"MONTH_NUMBER", "config":false}, 
                {"layout":"YEAR_WEEKS", "engine":"WEEK_NUMBER", "config":false},            
                {"layout":"MONTH_WEEKS", "engine":"WEEK_NUMBER", "config":false},  
                {"layout":"MONTH_DAYS", "engine":"DAY_NUMBER", "config":false},
                {"layout":"WEEK_DAYS", "engine":"DAY_NUMBER", "config":false},                
                {"layout":"DAY", "engine":"DAY_NUMBER", "config":false},
                {"layout":"DAY_HOURS",  "engine":"CELL_VALUE", "config":{"format":"HH:[00]"}},
                {"layout":"DAY_WORK_HOURS",  "engine":"CELL_VALUE", "config":{"format":"HH:[00]"}}
            ]
        }
    },
    "doc:OSE":"{{json _calendar}}"
}