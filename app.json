{
    "routing": {
        "routes": {
            "public/home": {
                "default:Templates.Home": {
                    "node:OML": [
                        {
                            "#home > .carousel:Carousel": {
                                "id": "carousel",
                                "dots": {
                                    "position": "top"
                                },
                                "node:OML": [
                                    {
                                        "default:Views.Login": {
                                            "id": "#login",
                                            "template": "Login"
                                        }
                                    },
                                    {
                                        "default:Views.Register": {
                                            "id": "#register",
                                            "template": "Register"
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                },
                "ctrl:Controllers.Home": {},
                "scope": [
                    "public"
                ]
            },
            "private/intro": {
                "default:Views.BigBang": {
                    "id": "#bigbang",
                    "template": "BigBang",
                    "css": "bigbang"
                },
                "scope": [
                    "public"
                ]
            },
            "private/settings": {
                "default:Views.Settings": {
                    "id": "#settings",
                    "template": "Settings",
                    "node:OML": {
                        "#theme_roul:Roulette": {
                            "id": "theme_roul",
                            "icon": true,
                            "list": [
                                {
                                    "label": "Dark",
                                    "value": true
                                },
                                {
                                    "label": "Light",
                                    "value": false
                                }
                            ],
                            "value": 0
                        }
                    }
                }
            },
            "private/object/object:([a-zA-Z]+)": {
                "default:Point": {
                    "id": "#point",
                    "live": [
                        "object",
                        "doc"
                    ],
                    "0-479": {
                        "winobj:Window": {
                            "title:OSE": "{{$object}}",
                            "width": "100%",
                            "height": "100%",
                            "anim": "right",
                            "node:OML": [
                                {
                                    "default:Templates.Doc": {
                                        "id": "#doc",
                                        "scroll": true,
                                        "data:OSE": "{{json crumb object}}"
                                    }
                                }
                            ]
                        },
                        "default:Views.Object": {
                            "id": "object",
                            "template": "Object",
                            "data:OSE": "{{json crumb object}}"
                        }
                    },
                    "480-5000": {
                        "default:Templates.Split": {
                            "node:OML": [
                                {
                                    "#point .split > .right:Templates.Doc": {
                                        "id": "#doc",
                                        "scroll": true,
                                        "data:OSE": "{{json crumb object}}"
                                    }
                                },
                                {
                                    "#point .split > .left:Views.Object": {
                                        "id": "object",
                                        "template": "Object",
                                        "data:OSE": "{{json crumb object}}"
                                    }
                                }
                            ]
                        }
                    }
                },
                "origin:Route": "self,private/intro,private/settings",
                "scope": [
                    "private"
                ]
            },
            "messenger/chat": {
                "default:Chat": {
                    "id": "chat",
                    "key": "_id"
                },
                "chatController:Controllers.Chat": {}
            },
            "sketch/draw": {
                "default:Views.Sketch": {
                    "id": "draw",
                    "template": "Sketch",
                    "observe": true,
                    "node:OML": {
                        "#sketch .controls:DynamicList": {
                            "id": "controls",
                            "key": "id",
                            "mode": "hit",
                            "list:OSE": "{{json controls}}",
                            "display": {
                                "html": "<span class=\"icon\"></span>",
                                "css": {
                                    "bind": "id",
                                    "add": "control"
                                }
                            }
                        },
                        "#sketch .colors:DynamicList": {
                            "id": "colors",
                            "key": "color",
                            "list:OSE": "{{json colors}}",
                            "display": {
                                "html": "<span class=\"icon\" style=\"background-color:{{$color}};\"></span>",
                                "css": "color"
                            }
                        },
                        "#sketch .brushes:DynamicList": {
                            "id": "brushes",
                            "key": "size",
                            "scroll": true,
                            "reorder": false,
                            "list:OSE": "{{json brushes}}",
                            "display": {
                                "template": "Brush",
                                "css": "brush"
                            }
                        }
                    }
                }
            }
        },
        "options": {
            "history": true,
            "bookmarkable": false
        },
        "errors": {}
    },
    "vapps": {
        "public:Stages.Public": {
            "template": "Public",
            "closeable": false,
            "use": true,
            "home": "home",
            "placeholder": "#main_public",
            "scope": [
                "public"
            ],
            "theater": false
        },
        "private:Stages.Private": {
            "template": "Private",
            "closeable": false,
            "home": "intro",
            "placeholder": "#main_private",
            "scope": [
                "public"
            ],
            "theater": {
                "icon": "ogx_logo.svg",
                "label": "Demo"
            },
            "node:OML": "{{oml menu}}"
        },
        "messenger:Stages.Messenger": {
            "template": "Chat",
            "closeable": true,
            "home": "chat",
            "placeholder": ".chat",
            "scope": [
                "public"
            ],
            "theater": {
                "icon": "messenger.svg",
                "label": "Messenger"
            }
        },
        "sketch:Stages.Sketch": {
            "template": "Basic",
            "closeable": true,
            "home": "draw",
            "placeholder": "#basic",
            "scope": [
                "public"
            ],
            "theater": {
                "icon": "sketch.svg",
                "label": "Sketch"
            }
        }
    },
